package thuchanh;

public class PhanSo {

	private int ts; 
	private int ms;
	public PhanSo() {
		ts = 0;
		ms = 1;
	}
	public PhanSo(int ts, int ms) {
		this.ts  = ts;
		this.ms  =ms;
	}
	public int getTs() {
		return ts;
	}
	public void setTs(int ts) {
		this.ts = ts;
	}
	public int getMs() {
		return ms;
	}
	public void setMs(int ms) {
		this.ms = ms;
	}
	
}
